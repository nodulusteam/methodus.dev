{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "console": "internalConsole",
            "name": "T Run express test",
            "type": "node",
            "program": "${workspaceRoot}/node_modules/alsatian/cli/alsatian-cli",
            "stopOnEntry": false,
            "args": [
                "build/tests/express.spec.js"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "env": {
                "PORT": "8020",
                "DEBUG": "none",
                "NODE_ENV": "debug",
                "NODE_LOG_LEVEL": "trace"
            }
        },
        {
            "request": "launch",
            "console": "internalConsole",
            "name": "T Run emitter test",
            "type": "node",
            "program": "${workspaceRoot}/node_modules/alsatian/cli/alsatian-cli",
            "stopOnEntry": false,
            "args": [
                "build/tests/emitter.spec.js"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "env": {
                "PORT": "8020",
                "DEBUG": "none",
                "NODE_ENV": "debug",
                "NODE_LOG_LEVEL": "trace"
            }
        },
        //REST
        {
            "type": "node",
            "request": "launch",
            "name": "* Rest caller",
            "program": "${workspaceRoot}/tests/callers/express.caller.js",
            "cwd": "${workspaceRoot}/",
            "console": "internalConsole",
            "outFiles": [],
            "sourceMaps": true,
            "env": {
                "TESTMODE": "run",
                "PORT": "8080",
                "DEBUG": "none",
                "NODE_LOG_LEVEL": "debug",
                "NODE_ENV": "debug",
                "TransportType": "Http"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "* Rest server",
            "program": "${workspaceRoot}/tests/servers/express.server.js",
            "cwd": "${workspaceRoot}/",
            "console": "internalConsole",
            "outFiles": [],
            "sourceMaps": true,
            "env": {
                "TESTMODE": "run",
                "PORT": "8090",
                "DEBUG": "none",
                "NODE_ENV": "debug",
                "NODE_LOG_LEVEL": "trace",
                "servers": "Express",
                "MethodType": "Local"
            }
        },
        {
            "request": "launch",
            "console": "internalConsole",
            // Name of configuration; appears in the launch configuration drop down menu.
            "name": "T Run current page test",
            // Type of configuration. Possible values: "node", "mono".
            "type": "node",
            // Workspace relative or absolute path to the program.
            "program": "${workspaceRoot}/node_modules/alsatian/cli/alsatian-cli",
            // Automatically stop program after launch.
            "stopOnEntry": false,
            // Command line arguments passed to the program.
            "args": [
                "${fileDirname}/${fileBasenameNoExtension}.js",
            ],
            "sourceMaps": true,
            // Workspace relative or absolute path to the working directory of the program being debugged. Default is the current workspace.
            "cwd": "${workspaceRoot}",
            // Workspace relative or absolute path to the runtime executable to be used. Default is the runtime executable on the PATH.
            "runtimeExecutable": null,
            // Environment variables passed to the program.
            "env": {
                "PORT": "8020",
                "DEBUG": "none",
                "NODE_ENV": "debug",
                "NODE_LOG_CONSOLE": "true"
            }
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858,
            "outFiles": [],
            "sourceMaps": true
        }
    ]
}